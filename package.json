{
  "name": "vballagregator",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "cross-env-shell \"DATABASE_URL=$DATABASE_URL\" prisma studio",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest src/tests/use-cases.test.ts",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest src/tests/integration.test.ts",
    "test:ci": "SKIP_INTEGRATION_TESTS=true NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "lint": "echo 'Linting not configured yet'"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@prisma/client": "^6.18.0",
    "@types/node": "^24.9.2",
    "@types/uuid": "^11.0.0",
    "bullmq": "^5.63.0",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0",
    "redis": "^5.9.0",
    "telegraf": "^4.16.3",
    "telegraf-ratelimit": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6"
  }
}
